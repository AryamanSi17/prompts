{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://prompts-server-drab.vercel.app/api/:path"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}