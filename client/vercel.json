{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "REPLACE_WITH_YOUR_BACKEND_URL/api/:path*"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}